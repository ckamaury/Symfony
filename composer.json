{
    "name": "ckamaury/symfony",
    "description": "mini-framework for Symfony",
    "type": "library",
    "license": "LGPL-3.0",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Romain FRANCOIS"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-intl": "*",
        "beberlei/doctrineextensions": "^1.3",
        "ckamaury/spreadsheet": "^1.0",
        "ckamaury/phpcurl": "^1.0",
        "symfony/apache-pack": "^1.0",
        "symfony/webapp-pack": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "CkAmaury\\Symfony\\": "src"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3"
    }
}
